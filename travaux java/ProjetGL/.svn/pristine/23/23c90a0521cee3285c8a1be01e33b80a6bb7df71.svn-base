		this.x = X;
		this.y = Y;
		this.traversabiliteObstacle = traversabiliteObstacle;

	}

	public Case() {
		super();
	}

	public Case(int i, int j) {
		this.x = i;
		this.y = j;
		// TODO Auto-generated constructor stub
	}

	public void setTraversabiliteObstacle(boolean traversabiliteObstacle) {
		this.traversabiliteObstacle = traversabiliteObstacle;
	}

