package genie;

public class Utilisateur extends Gardien {
	
	private int x1;
	private int y1;
	
	public Utilisateur(int x, int y,int x1, int y1) {
		
		super(x, y);
		this.x1 = x1;
		this.y1 = y1;
		
	}

	public int getX1() {
		return x1;
	}

	public void setX1(int x1) {
		this.x1 = x1;
	}

	public int getY1() {
		return y1;
	}

	public void setY1(int y1) {
		this.y1 = y1;
	}

	@Override
	public String toString() {
		return "Utilisateur [x1=" + x1 + ", y1=" + y1 + " "+ super.toString() + "]";
	}
	
	
	
	
	
	
	
	
	
	
	

}
